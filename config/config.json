{
  "$schema": "https://developer.microsoft.com/json-schemas/spfx-build/config.2.0.schema.json",
  "version": "2.0",
  "bundles": {
    "social-links-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/socialLinks/SocialLinksWebPart.js",
          "manifest": "./src/webparts/socialLinks/SocialLinksWebPart.manifest.json"
        }
      ]
    },
    "flip-clock-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/flipClock/FlipClockWebPart.js",
          "manifest": "./src/webparts/flipClock/FlipClockWebPart.manifest.json"
        }
      ]
    }
  },
  "externals": {},
  "localizedResources": {
    "SocialLinksWebPartStrings": "lib/webparts/socialLinks/loc/{locale}.js",
    "FlipClockWebPartStrings": "lib/webparts/flipClock/loc/{locale}.js",
    "PropertyControlStrings": "node_modules/@pnp/spfx-property-controls/lib/loc/{locale}.js"
  }
}